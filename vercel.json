{
  "builds" : [
    {
      "src": "backend_class/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src":"/(.*)",
      "dest": "backend_class/wsgi.py"
    }
  ],  
  "rewrites": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
